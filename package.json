{
  "name": "@vetaljanos/n8n-nodes-aws-sqs-trigger",
  "version": "1.2.1",
  "description": "Consume queue messages from AWS SQS in n8n",
  "keywords": [
    "n8n-community-node-package",
    "n8n-nodes",
    "aws",
    "sqs",
    "trigger"
  ],
  "license": "MIT",
  "homepage": "https://github.com/matthewkayy",
  "author": {
    "name": "Matthew Kay",
    "email": "matthewkay@live.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matthewkayy/n8n-nodes-aws-sqs-trigger.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "commit": "git-cz",
    "lint": "tslint -p tsconfig.json -c tslint.json && eslint nodes package.json",
    "lintfix": "tslint --fix -p tsconfig.json -c tslint.json && eslint nodes package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes package.json",
    "semantic-release": "semantic-release"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/AwsSqsTrigger/AwsSqsTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@types/aws4": "^1.11.6",
    "@types/express": "^5.0.5",
    "@types/request-promise-native": "~1.0.21",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/parser": "^8.46.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.4",
    "gulp": "^5.0.1",
    "n8n-core": "^1.117.1",
    "n8n-workflow": "^1.116.0",
    "prettier": "^2.7.1",
    "semantic-release": "^25.0.1",
    "tslint": "^6.1.3",
    "typescript": "~5.9.3"
  },
  "dependencies": {
    "aws4": "^1.13.2",
    "xml2js": "^0.6.2"
  },
  "bugs": {
    "url": "https://github.com/matthewkayy/n8n-nodes-aws-sqs-trigger/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      "master",
      "next"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
